{
  "name": "@art70x/prewind",
  "version": "1.0.0",
  "description": "A lightweight preprocessor that expands Tailwind shorthand like hover(px-4 py-2) into hover:px-4 hover:py-2.",
  "keywords": [
    "cli",
    "utility",
    "preprocessor",
    "shorthand",
    "tailwindcss",
    "art70x"
  ],
  "homepage": "https://github.com/art70x/Prewind#readme",
  "bugs": {
    "url": "https://github.com/art70x/Prewind/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/art70x/Prewind.git"
  },
  "license": "MIT",
  "author": "art70x",
  "type": "module",
  "bin": {
    "prewind": "dist/cli.js"
  },
  "files": [
    "dist/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "check": "tsc",
    "dev": "tsx src/cli.ts",
    "format": "prettier -w src",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "standard-version"
  },
  "dependencies": {
    "cli-spinners": "^3.3.0",
    "commander": "^14.0.2",
    "fast-glob": "^3.3.3",
    "yocto-spinner": "^1.0.0",
    "yoctocolors": "^2.1.2"
  },
  "devDependencies": {
    "@antfu/ni": "^27.0.1",
    "@eslint/js": "^9.39.0",
    "@types/node": "^24.9.2",
    "eslint": "^9.39.0",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "standard-version": "^9.5.0",
    "terser": "^5.44.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "packageManager": "pnpm@10.20.0"
}
